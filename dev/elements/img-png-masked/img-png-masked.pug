mixin img-png(imgUrl)
    svg(width="100%" height="100%" viewBox="0 0 50 50" id="mask-svg")
        defs
            mask(id="theMask")
                rect(width="100%" height="100%" fill="#000")
                polygon(points="25,1.061 37.601,10.217 50.203,19.372 45.39,34.186 40.576,49 25,49 9.424,49 4.61,34.186 -0.203,19.372 12.399,10.217 " fill="#fff")
        image(width='50' height='50' href=imgUrl mask='url(#theMask)')
